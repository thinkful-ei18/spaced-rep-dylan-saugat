{
  "name": "spaced-rep-backend",
  "version": "0.1.0",
  "description": "Express back end for spaced repetition project",
  "main": "index.js",
  "scripts": {
    "start": "nodemon index.js",
    "mocha": "mocha --exit --file test/setup.test.js '!(node_modules)/**/*test.js'",
    "mocha-exit0": "mocha --exit --file test/setup.test.js '!(node_modules)/**/*test.js' || true",
    "test": "npm run mocha",
    "cover": "nodemon --exec nyc --reporter=lcov --reporter=text-summary npm run mocha-exit0",
    "heroku": "node index.js",
    "travis": "npm run mocha"
  },
  "author": "Dylan Nielsen & Saugat Dhungel",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "chai-spies": "^1.0.0",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.2.1",
    "knex": "^0.14.4",
    "mongoose": "^5.0.6",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.4.1",
    "sinon": "^4.5.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "mocha": "^5.0.4",
    "nyc": "^11.6.0"
  }
}
